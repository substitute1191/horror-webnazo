{
  "include": ["src/**/*", "jest.setup.ts"],
  "exclude": [],
  "compilerOptions": {
    "target": "ES2022",
    "module": "esnext",
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"]
    },
    "strict": true,
    "noFallthroughCasesInSwitch": true
  }
}
